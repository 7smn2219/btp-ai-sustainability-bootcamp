{
    "_version": "1.8.0",
    "sap.app": {
        "id": "soundanomalytypes",
        "type": "application",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "i18n": "i18n/i18n.properties",
        "dataSources": {
            "AdminService": {
                "uri": "/admin/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "-sourceTemplate": {
            "id": "ui5template.basicSAPUI5ApplicationProject",
            "-id": "ui5template.smartTemplate",
            "-version": "1.40.12"
        }
    },
    "sap.ui5": {
        "dependencies": {
            "libs": {
                "sap.fe.templates": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "AdminService",
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect" : true,
                    "earlyRequests": true,
                    "groupProperties": {
                        "default": {
                            "submit": "Auto"
                        }
                    }
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "SoundAnomalyTypesList",
                    "target": "SoundAnomalyTypesList"
                },
                {
                    "pattern": "SoundAnomalyTypes({key}):?query:",
                    "name": "SoundAnomalyTypesDetails",
                    "target": "SoundAnomalyTypesDetails"
                }
            ],
            "targets": {
                "SoundAnomalyTypesList": {
                    "type": "Component",
                    "id": "SoundAnomalyTypesList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings" : {
                            "entitySet" : "SoundAnomalyTypes",
                            "initialLoad": true,
                            "navigation" : {
                                "SoundAnomalyTypes" : {
                                    "detail" : {
                                        "route" : "SoundAnomalyTypesDetails"
                                    }
                                }
                            }
                        }
                    }
                },
                "SoundAnomalyTypesDetails": {
                    "type": "Component",
                    "id": "SoundAnomalyTypesDetailsList",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings" : {
                            "entitySet" : "SoundAnomalyTypes",
                            "navigation" : {
                                "SoundAnomalyTypes" : {
                                    "detail" : {
                                        "route" : "SoundAnomalyTypesDetails"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "fullWidth": false
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}