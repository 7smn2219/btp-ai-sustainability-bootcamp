{
    "_version": "1.8.0",
    "sap.app": {
        "id": "anomalies",
        "type": "application",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "i18n": "i18n/i18n.properties",
        "dataSources": {
            "AdminService": {
                "uri": "/admin/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "-sourceTemplate": {
            "id": "ui5template.basicSAPUI5ApplicationProject",
            "-id": "ui5template.smartTemplate",
            "-version": "1.40.12"
        }
    },
    "sap.ui5": {
        "dependencies": {
            "libs": {
                "sap.fe.templates": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "AdminService",
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect" : true,
                    "earlyRequests": true,
                    "groupProperties": {
                        "default": {
                            "submit": "Auto"
                        }
                    }
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "AnomaliesList",
                    "target": "AnomaliesList"
                },
                {
                    "pattern": "Anomalies({key}):?query:",
                    "name": "AnomaliesDetails",
                    "target": "AnomaliesDetails"
                },
                {
                    "pattern": "Anomalies({key}/anomalyType({key2}):?query:",
                    "name": "AnomalyTypesDetails",
                    "target": "AnomalyTypesDetails"
                }
            ],
            "targets": {
                "AnomaliesList": {
                    "type": "Component",
                    "id": "AnomaliesList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings" : {
                            "entitySet" : "Anomalies",
                            "initialLoad": true,
                            "navigation" : {
                                "Anomalies" : {
                                    "detail" : {
                                        "route" : "AnomaliesDetails"
                                    }
                                }
                            }
                        }
                    }
                },
                "AnomaliesDetails": {
                    "type": "Component",
                    "id": "AnomaliesDetailsList",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings" : {
                            "entitySet" : "Anomalies",
                            "navigation" : {
                                "AnomalyTypes" : {
                                    "detail" : {
                                        "route" : "AnomalyTypesDetails"
                                    }
                                }
                            },
                            "content": {
								"body": {
									"sections": {
										"customAudioSection": {
											"type": "XMLFragment",
											"name": "anomalies.custom.CustomAudioSection",
											"title": "{i18n>customAudioSection}",
											"position": {
												"placement": "After",
												"anchor": "Details"
											}
										}
									}
                                }
                            }
                        }
                    }
                },
                "AnomalyTypesDetails": {
                    "type": "Component",
                    "id": "AnomalyTypesDetailsList",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings" : {
                            "entitySet" : "AnomalyTypes"
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "fullWidth": false
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}